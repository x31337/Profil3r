{
    "config_path": "./config.prod.json",
    "separators": {
        "Dot": ".",
        "Dash": "-",
        "Underscore": "_"
    },
    "report_elements": [
        "email",
        "facebook",
        "twitter",
        "instagram",
        "github",
        "linkedin"
    ],
    "json_report_path": "./reports/prod/json/{}.json",
    "html_report_path": "./reports/prod/html/{}.html",
    "csv_report_path": "./reports/prod/csv/{}.csv",
    "profil3r": {
        "version": "1.3.11",
        "timeout": 15,
        "max_workers": 50,
        "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36",
        "retry_count": 5
    },
    "language_settings": {
        "python": {
            "packages": ["requests", "beautifulsoup4", "lxml"],
            "virtual_env": "/opt/venv"
        },
        "javascript": {
            "packages": ["axios", "cheerio"],
            "node_version": "18.x"
        },
        "php": {
            "packages": ["guzzlehttp/guzzle"],
            "php_version": "8.1"
        }
    },
    "logging": {
        "level": "WARNING",
        "file": "./logs/prod/profil3r.log",
        "max_size": 10485760,
        "backup_count": 10
    },
    "plateform": {
        "domain": {
            "rate_limit": 200,
            "format": "http://{permutation}.{domain}",
            "TLD": ["org", "com", "net", "io", "co"],
            "type": "domain",
            "enabled": "yes"
        },
        "email": {
            "rate_limit": 300,
            "domains": [
                "gmail.com",
                "yahoo.com",
                "hotmail.com",
                "outlook.com",
                "protonmail.com"
            ],
            "format": "{permutation}@{domain}",
            "type": "email",
            "enabled": "yes"
        },
        "facebook": {
            "rate_limit": 200,
            "format": "https://facebook.com/{permutation}",
            "type": "social",
            "enabled": "yes"
        },
        "twitter": {
            "rate_limit": 300,
            "format": "https://twitter.com/{permutation}",
            "type": "social",
            "enabled": "yes"
        },
        "instagram": {
            "rate_limit": 200,
            "format": "https://instagram.com/{permutation}",
            "type": "social",
            "enabled": "yes"
        },
        "github": {
            "rate_limit": 200,
            "format": "https://github.com/{permutation}",
            "type": "programming",
            "enabled": "yes"
        },
        "linkedin": {
            "rate_limit": 150,
            "format": "https://linkedin.com/in/{permutation}",
            "type": "social",
            "enabled": "yes"
        }
    }
}
