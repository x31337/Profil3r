{
    "config_path": "./config.ci.json",
    "separators": {
        "Dot": ".",
        "Dash": "-",
        "Underscore": "_"
    },
    "report_elements": [
        "email",
        "facebook",
        "twitter",
        "github"
    ],
    "json_report_path": "./reports/ci/json/{}.json",
    "html_report_path": "./reports/ci/html/{}.html",
    "csv_report_path": "./reports/ci/csv/{}.csv",
    "profil3r": {
        "version": "1.3.11",
        "timeout": 30,
        "max_workers": 20,
        "user_agent": "Mozilla/5.0 (CI) AppleWebKit/537.36",
        "retry_count": 3
    },
    "language_settings": {
        "python": {
            "packages": ["requests", "beautifulsoup4", "lxml", "pytest"],
            "virtual_env": "/opt/venv"
        },
        "javascript": {
            "packages": ["axios", "cheerio", "jest"],
            "node_version": "18.x"
        }
    },
    "logging": {
        "level": "INFO",
        "file": "./logs/ci/profil3r.log",
        "max_size": 5242880,
        "backup_count": 2
    },
    "plateform": {
        "email": {
            "rate_limit": 100,
            "domains": ["gmail.com", "yahoo.com", "hotmail.com"],
            "format": "{permutation}@{domain}",
            "type": "email",
            "enabled": "yes"
        },
        "facebook": {
            "rate_limit": 100,
            "format": "https://facebook.com/{permutation}",
            "type": "social",
            "enabled": "yes"
        },
        "twitter": {
            "rate_limit": 100,
            "format": "https://twitter.com/{permutation}",
            "type": "social",
            "enabled": "yes"
        },
        "github": {
            "rate_limit": 100,
            "format": "https://github.com/{permutation}",
            "type": "programming",
            "enabled": "yes"
        }
    }
}
